{
  "name": "user-scripts",
  "version": "1.0.0",
  "description": "User scripts for Tampermonkey",
  "main": "index.js",
  "scripts": {
    "build": "node .build/build.js",
    "build:ple": "npm run build -- -s=ple && npm run fix",
    "build:jrdb": "npm run build -- -s=jrdb && npm run fix",
    "build:pleng": "npm run build -- -s=pleng && npm run fix",
    "build:iv": "npm run build -- -s=iv && npm run fix",
    "watch": "node .build/build.js --watch",
    "watch:ple": "npm run watch -- -s=ple",
    "watch:jrdb": "npm run watch -- -s=jrdb",
    "watch:pleng": "npm run watch -- -s=pleng",
    "watch:iv": "npm run watch -- -s=iv",
    "fix": "eslint dist/*.user.js --fix"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shikiyoku/user-scripts.git"
  },
  "keywords": [
    "user.js",
    "userscript"
  ],
  "author": "shikiyoku",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/tampermonkey": "^4.0.0",
    "blissfuljs": "^1.0.6",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "postcss": "^7.0.26",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-properties": "^9.0.2",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^4.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-userscript-metablock": "^0.2.2",
    "stylelint": "^13.0.0",
    "stylelint-config-property-sort-order-smacss": "^5.2.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "yargs": "^15.1.0"
  },
  "dependencies": {}
}
