{
  "name": "user-scripts",
  "version": "1.0.0",
  "description": "User scripts for Tampermonkey",
  "main": "index.js",
  "scripts": {
    "build": "node build/build.js",
    "build:ple": "npm run build -- -s=ple && npm run fix -- dist/pornolab-enhancer.user.js",
    "build:jrdb": "npm run build -- -s=jrdb && npm run fix -- dist/joyreactor-download-button.user.js",
    "build:pleng": "npm run build -- -s=pleng && npm run fix -- dist/pornolab-english.user.js",
    "build:iv": "npm run build -- -s=iv && npm run fix -- dist/image-viewer.user.js",
    "watch": "node build/build.js --watch",
    "watch:ple": "npm run watch -- -s=ple",
    "watch:jrdb": "npm run watch -- -s=jrdb",
    "watch:pleng": "npm run watch -- -s=pleng",
    "watch:iv": "npm run watch -- -s=iv",
    "fix": "eslint --fix"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shikiyoku/user-scripts.git"
  },
  "keywords": [
    "user.js",
    "userscript"
  ],
  "author": "shikiyoku",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/tampermonkey": "^4.0.0",
    "blissfuljs": "^1.0.6",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "postcss": "^7.0.32",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^4.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.15.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-userscript-metablock": "^0.2.5",
    "stylelint": "^13.6.0",
    "stylelint-config-property-sort-order-smacss": "^6.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "yargs": "^15.3.1"
  },
  "dependencies": {}
}
