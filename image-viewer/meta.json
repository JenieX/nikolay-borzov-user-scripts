{
  "name": "Image Viewer",
  "version": "1.2.1",
  "description": "View full image without leaving the page",
  "icon": "https://raw.githubusercontent.com/nikolay-borzov/user-scripts/master/image-viewer/icon.png",
  "match": "*://*/*",
  "connect": [
    "imagebam.com",
    "imagevenue.com",
    "www.turboimagehost.com",
    "fastpic.ru",
    "fastpic.org",
    "radikal.ru",
    "imagetwist.com"
  ],
  "grant": [
    "GM_addStyle",
    "GM_xmlhttpRequest",
    "GM.xmlHttpRequest",
    "GM_setValue",
    "GM.setValue",
    "GM_getValue",
    "GM.getValue",
    "GM_registerMenuCommand",
    "GM.registerMenuCommand"
  ],
  "run-at": "document-start"
}
